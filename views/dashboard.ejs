<body class="">
    <div class="wrapper ">
        <div class="sidebar" data-color="orange">
            <!--
        Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
    -->
            <div class="logo">
                
                    Traffic Management System
                </a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li class="active">
                        <a href="/dashboard">
                            <i class="now-ui-icons design_app"></i>
                            <p>Average Traffic Density</p>
                        </a>
                    </li>
                    <li>
                        <a href="/maps">
                            <i class="now-ui-icons location_map-big"></i>
                            <p>Maps</p>
                        </a>
                    </li>
                    <li>
                        <a href="/tables">
                            <i class="now-ui-icons design_bullet-list-67"></i>
                            <p>Table List</p>
                        </a>
                    </li>
                    <li>
                        <center style="margin-top:20%;">
                            <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal">Find</button>
                        </center>
                    </li>
                    
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-transparent  navbar-absolute bg-primary fixed-top">
                <div class="container-fluid">
                    <div class="navbar-wrapper">
                        <div class="navbar-toggle">
                            <button type="button" class="navbar-toggler">
                                <span class="navbar-toggler-bar bar1"></span>
                                <span class="navbar-toggler-bar bar2"></span>
                                <span class="navbar-toggler-bar bar3"></span>
                            </button>
                        </div>
                        <a class="navbar-brand" href="#pablo">Average Traffic Density</a>
                    </div>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-bar navbar-kebab"></span>
                        <span class="navbar-toggler-bar navbar-kebab"></span>
                        <span class="navbar-toggler-bar navbar-kebab"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end" id="navigation">
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->
            <div style="margin-left: 0%;" id='myChart'><a class="zc-ref" href="#"></a>
            </div>
            
            <div class="container">
                
                <!-- Trigger the modal with a button -->
                
              
                <!-- Modal -->
                <div class="modal fade" id="myModal" role="dialog">
                  <div class="modal-dialog">
                  
                    <!-- Modal content-->
                    <div class="modal-content">
                     
                      <div class="modal-body">
                        <form>
                            <center><h4 class="modal title">Track your Travel</h4></center>
                            <div class="form-group">
                              <label for="usr">From:</label>
                              <input type="text" class="form-control" id="usr">
                            </div>
                            <div class="form-group">
                              <label for="pwd">To:</label>
                              <input type="text" class="form-control" id="pwd">
                            </div>
                          </form>
                        
                      </div>
                      <div class="modal-footer">
                     <button   type="button" class="btn btn-default">Search</button>
                      </div>
                    </div>
                    
                  </div>
                </div>
                
              </div>
              
              

        </div>
    </div>
</body>
